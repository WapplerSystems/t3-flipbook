[{$plugin.tx_flipbook.settings.includeJqueryHeader} == 1]
    page.includeJSlibs.jQueryRflipBook = EXT:flipbook/Resources/Public/JavaScript/jquery-3.4.1.min.js
    page.includeJSlibs.jQueryRflipBook.excludeFromConcatenation = 1
    page.includeJSlibs.jQueryRflipBook.disableCompression = 1
[END]

[{$plugin.tx_flipbook.settings.includeJqueryFooter} == 1]
    page.includeJSFooterlibs.jQueryRflipBook = EXT:flipbook/Resources/Public/JavaScript/jquery-3.4.1.min.js
    page.includeJSFooterlibs.jQueryRflipBook.excludeFromConcatenation = 1
    page.includeJSFooterlibs.jQueryRflipBook.disableCompression = 1
[END]

page.includeCSS {

    flipbook = EXT:flipbook/Resources/Public/CSS/flipbook.style.css
    flipbook.excludeFromConcatenation = 1
    flipbook.disableCompression = 1

    #flipbookFa = EXT:flipbook/Resources/Public/CSS/font-awesome.css
    #flipbookFa.excludeFromConcatenation = 1
    #flipbookFa.disableCompression = 1

}
